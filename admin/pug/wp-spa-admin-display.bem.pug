include _auto-generated/_-all
:php
    /**
    * Provide a admin area view for the plugin
    *
    * This file is used to markup the admin-facing aspects of the plugin.
    *
    * @link       khalidhoffman.solutions
    * @since      1.0.0
    *
    * @package    Wp_Spa
    * @subpackage Wp_Spa/admin/partials
    */
#wp-spa
    .heading
        h1._content!=echo("esc_html(get_admin_page_title());")
    .settings
        ._content
            .setting-section.-vars.-left
                ._content
                    form.setting-form(action="options.php" method="post")
                        :php
                            settings_fields($this->plugin_name);
                            do_settings_sections($this->plugin_name);
                            submit_button();
            .setting-section.-demo.-right
                .wpspa-demo
                    .__heading
                        h2 Demo
                    .__content
                        .__view
                            .__demo_el
                        select.__setting()
    .feedback
        .__heading
            h2 Send Feedback
        .__content
            textarea.__input()
            a.button.button-primary.-send Send
